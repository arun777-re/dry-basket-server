  "main": "index.js",
"scripts": {
  "build": "tsc",
  "start": "node dist/server.js",
  "dev": "nodemon --legacy-watch --exec ts-node ./src/server.ts",
  
  "worker:order": "ts-node -r dotenv/config src/workers/orderWorker.ts",
  "worker:order:prod": "node dist/workers/orderWorker.js",

  "worker:order-cancel": "ts-node -r dotenv/config src/workers/orderCancelWorker.ts",
  "worker:order-cancel:prod": "node dist/workers/orderCancelWorker.js",

  "worker:order-track": "ts-node -r dotenv/config src/workers/orderTrackingWorker.ts",
  "worker:order-track:prod": "node dist/workers/orderTrackingWorker.js"
}
